{
  "name": "Chrome_auto",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chromeの自動操作をやりたい",
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "js/utils.js",
        "js/datas.js",
        "js/setAuto.js",
        "js/setConsole.js",
        "js/cmdwin/parser.js",
        "js/cmdwin/createCmdWin.js"
      ]
    }
  ]
}
