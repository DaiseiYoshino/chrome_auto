{
  "name": "Chrome_auto",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chromeの自動操作をやりたい",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "utils.js",
        "datas.js",
        "main.js",
        "cmdwin/createCmdWin.js"
      ]
    }
  ]
}
