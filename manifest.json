{
  "name": "Chrome_auto",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Chromeの自動操作をやりたい",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "downloads"
  ],
  "background": [
    {
      "service_worker": [
        "lib/utils.js",
        "lib/cmdwin/createCmdWin.js",
        "lib/cmdwin/parser.js",
        "lib/main.js"
      ],
      "type": "module"
    },
    {
      "service_worker": "lib/screenshot.js"
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "lib/datas.js",
        "lib/setAuto.js",
        "run.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["*://*/*"],
      "resources": [
        "lib/cmdwin/createCmdWin.js",
        "lib/cmdwin/parser.js",
        "lib/setConsole/setConsole.js",
        "lib/setConsole/basic.js",
        "lib/main.js"
      ]
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self'; object-src 'self'; script-src 'self';"
  }
}
